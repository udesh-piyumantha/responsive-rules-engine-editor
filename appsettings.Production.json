{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "RulesEngineEditor": "Information"
    }
  },
  "Storage": {
    "Type": "AzureBlob",
    "JsonFilePath": "D:\\RulesStorage",
    "S3": {
      "BucketName": "prod-rules-engine-bucket",
      "Prefix": "workflows/"
    },
    "Azure": {
      "ConnectionString": "${AZURE_STORAGE_CONNECTION_STRING}",
      "ContainerName": "workflows-prod"
    }
  },
  "AWS": {
    "AccessKeyId": "${AWS_ACCESS_KEY_ID}",
    "SecretAccessKey": "${AWS_SECRET_ACCESS_KEY}",
    "Region": "us-east-1"
  },
  "DetailedErrors": false,
  "Https": {
    "Enabled": true,
    "Port": 443
  }
}
